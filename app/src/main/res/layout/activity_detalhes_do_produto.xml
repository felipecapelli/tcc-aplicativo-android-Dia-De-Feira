<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/activity_detalhes_do_produto_id_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:text="id_feira" />

    <TextView
        android:id="@+id/activity_detalhes_do_produto_nome_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nome_feira" />

    <TextView
        android:id="@+id/activity_detalhes_do_produto_endereco_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="endereco_feira" />

    <TextView
        android:id="@+id/activity_detalhes_do_produto_data_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="data_feira" />

    <ListView
        android:id="@+id/activity_detalhes_do_produto_lista_detalhes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


</LinearLayout>
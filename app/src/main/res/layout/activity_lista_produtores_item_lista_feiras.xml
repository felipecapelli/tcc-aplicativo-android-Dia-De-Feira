<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/item_lista_produtores_titulo_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Feira"
        android:textStyle="bold"
        android:paddingLeft="10dp"
        android:paddingTop="8dp"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/item_lista_produtores_id_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Id do produtores"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/item_lista_produtores_nome_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome da feira"
        android:paddingLeft="10dp"
        android:paddingTop="8dp"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/item_lista_produtores_data_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data da feira"
        android:paddingLeft="10dp"
        android:paddingTop="8dp"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/item_lista_produtores_endereco_feira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EndereÃ§o da feira"
        android:paddingLeft="10dp"
        android:paddingTop="8dp"
        android:textSize="20sp"/>

</LinearLayout>
